<style>
	<shape><name>default</name><path>
		<moveto><x>-1</x><y>-2</y></moveto>
		<lineto><x>1</x><y>-2</y></lineto>
		<curveto>
			<p1><x>1.5</x><y>-2</y></p1>
			<p2><x>2</x><y>-1.5</y></p2>
			<p3><x>2</x><y>-1</y></p3>
		</curveto>
		<lineto><x>2</x><y>1</y></lineto>
		<curveto>
			<p1><x>2</x><y>1.5</y></p1>
			<p2><x>1.5</x><y>2</y></p2>
			<p3><x>1</x><y>2</y></p3>
		</curveto>
		<lineto><x>-1</x><y>2</y></lineto>
		<curveto>
			<p1><x>-1.5</x><y>2</y></p1>
			<p2><x>-2</x><y>1.5</y></p2>
			<p3><x>-2</x><y>1</y></p3>
		</curveto>
		<lineto><x>-2</x><y>-1</y></lineto>
		<curveto>
			<p1><x>-2</x><y>-1.5</y></p1>
			<p2><x>-1.5</x><y>-2</y></p2>
			<p3><x>-1</x><y>-2</y></p3>
		</curveto>
	</path></shape>
	<shape><name>Return</name><path>
		<moveto><x>-2</x><y>-0.5</y></moveto>
		<lineto><x>-2</x><y>-1.5</y></lineto>
		<curveto>
			<p1><x>-2</x><y>-1.75</y></p1>
			<p2><x>-1.5</x><y>-2</y></p2>
			<p3><x>-1</x><y>-2</y></p3>
		</curveto>
		<lineto><x>1</x><y>-2</y></lineto>
		<curveto>
			<p1><x>1.5</x><y>-2</y></p1>
			<p2><x>2</x><y>-1.5</y></p2>
			<p3><x>2</x><y>-1</y></p3>
		</curveto>
		<lineto><x>2</x><y>1</y></lineto>
		<curveto>
			<p1><x>2</x><y>1.5</y></p1>
			<p2><x>1.5</x><y>2</y></p2>
			<p3><x>1</x><y>2</y></p3>
		</curveto>
		<lineto><x>0</x><y>2</y></lineto>
		<curveto>
			<p1><x>-0.333</x><y>2</y></p1>
			<p2><x>-0.667</x><y>1.5</y></p2>
			<p3><x>-0.667</x><y>1</y></p3>
		</curveto>
		<lineto><x>-0.667</x><y>0.5</y></lineto>
		<curveto>
			<p1><x>-0.667</x><y>0.25</y></p1>
			<p2><x>-0.75</x><y>0</y></p2>
			<p3><x>-1</x><y>0</y></p3>
		</curveto>
		<lineto><x>-1.5</x><y>0</y></lineto>
		<curveto>
			<p1><x>-1.75</x><y>0</y></p1>
			<p2><x>-2</x><y>-0.25</y></p2>
			<p3><x>-2</x><y>-0.5</y></p3>
		</curveto>
