	<symbol>
		<name>(KP_|)Home</name>
		<draw><arrow>
			<p><x>0</x><y>0</y></p>
			<p><x>-1</x><y>-1</y></p>
		</arrow></draw>
	</symbol>
	<symbol>
		<name>(KP_|)Page_Up</name>
		<draw><arrow>
			<p><x>0</x><y>0.6</y></p>
			<p><x>0</x><y>-1</y></p>
		</arrow>
		<line>
