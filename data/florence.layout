	</symbol>
	<symbol>
		<name>ISO_Level3_Shift</name>
		<label>altGr</label>
	</symbol>
	<symbol>
		<name>BackSpace</name>
		<draw><arrow>
			<p><x>1</x><y>0</y></p>
			<p><x>-1</x><y>0</y></p>
		</arrow></draw>
	</symbol>
	<symbol>
		<name>Tab</name>
		<draw>
			<arrow><p><x>-0.75</x><y>0</y></p>
			<p><x>0.5</x><y>0</y></p></arrow>
			<line><p><x>0.75</x><y>-0.5</y></p>
			<p><x>0.75</x><y>0.5</y></p></line>
		</draw>
	</symbol>
	<symbol>
		<name>ISO_Left_Tab</name>
		<draw>
			<arrow><p><x>0.75</x><y>0</y></p>
			<p><x>-0.5</x><y>0</y></p></arrow>
			<line><p><x>-0.75</x><y>-0.5</y></p>
			<p><x>-0.75</x><y>0.5</y></p></line>
		</draw>
	</symbol>
	<symbol>
		<name>Return</name>
		<draw><arrow>
			<p><x>1.333</x><y>-2.125</y></p>
			<p><x>1.333</x><y>-1.625</y></p>
			<p><x>-1.333</x><y>-1.625</y></p>
		</arrow></draw>
	</symbol>
	<symbol>
		<name>Caps_Lock</name>
		<draw>
			<rect><p1><x>-0.5</x><y>-0.25</y></p1>
			<p2><x>0.5</x><y>0.75</y></p2></rect>
			<path>
				<moveto><x>-0.25</x><y>-0.25</y></moveto>
				<lineto><x>-0.25</x><y>-0.5</y></lineto>
				<curveto>
					<p1><x>-0.25</x><y>-0.625</y></p1>
					<p2><x>-0.125</x><y>-0.75</y></p2>
					<p3><x>0</x><y>-0.75</y></p3>
				</curveto>
				<curveto>
					<p1><x>0.125</x><y>-0.75</y></p1>
					<p2><x>0.25</x><y>-0.625</y></p2>
					<p3><x>0.25</x><y>-0.5</y></p3>
				</curveto>
				<lineto><x>0.25</x><y>-0.25</y></lineto>
			</path>
		</draw>
	</symbol>
	<symbol>
		<name>(Shift_[LR]|(KP_|)Up)</name>
		<draw><arrow>
			<p><x>0</x><y>0.5</y></p>
			<p><x>0</x><y>-0.5</y></p>
		</arrow></draw>
