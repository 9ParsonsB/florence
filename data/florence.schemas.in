<gconfschemafile>
	<schemalist>

		<schema>
			<key>/schemas/apps/florence/behaviour/auto_hide</key>
			<applyto>/apps/florence/behaviour/auto_hide</applyto>
			<owner>florence</owner>
			<type>bool</type>
			<default>false</default>
			<locale name="C">
				<short>Hide the keyboard automatically when not needed</short>
				<long>Florence will hide automatically when no editable widget is selected.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/behaviour/move_to_widget</key>
			<applyto>/apps/florence/behaviour/move_to_widget</applyto>
			<owner>florence</owner>
			<type>bool</type>
			<default>false</default>
			<locale name="C">
				<short>Move the keyboard to near the selected widget</short>
				<long>Move the keyboard to near the selected editable widget in auto-hide mode. Only useful when auto_hide is activated.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/behaviour/intermediate_icon</key>
			<applyto>/apps/florence/behaviour/intermediate_icon</applyto>
			<owner>florence</owner>
			<type>bool</type>
			<default>true</default>
			<locale name="C">
				<short>Show an intermediate icon before showing the keyboard in auto-hide mode.</short>
				<long>Show an intermediate icon when you click on an editable widget. Click on the icon to show the actual keyboard.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/behaviour/input_method</key>
			<applyto>/apps/florence/behaviour/input_method</applyto>
			<owner>florence</owner>
			<type>string</type>
			<default>button</default>
			<locale name="C">
				<short>Input method</short>
				<long>Set the input method. Available input methods are button, timer and ramble.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/behaviour/timer</key>
			<applyto>/apps/florence/behaviour/timer</applyto>
			<owner>florence</owner>
			<type>float</type>
			<default>1300</default>
			<locale name="C">
				<short>Auto click timeout</short>
				<long>Time, in milliseconds, to wait when the mouse is over a key before the key is automatically pressed (when timer input method is selected)</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/behaviour/ramble_algo</key>
			<applyto>/apps/florence/behaviour/ramble_algo</applyto>
			<owner>florence</owner>
			<type>string</type>
			<default>distance</default>
			<locale name="C">
				<short>Ramble algorithm</short>
				<long>Set the ramble algorithm used. Valid algorithms are distance and time.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/behaviour/ramble_threshold1</key>
			<applyto>/apps/florence/behaviour/ramble_threshold1</applyto>
			<owner>florence</owner>
			<type>float</type>
			<default>1.3</default>
			<locale name="C">
				<short>Distance threshold for distance based ramble mode for first key press.</short>
				<long>When ramble input method is selected, this the distance the pointer must move over a key (relative to the size of the key) before the key is pressed</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/behaviour/ramble_threshold2</key>
			<applyto>/apps/florence/behaviour/ramble_threshold2</applyto>
			<owner>florence</owner>
			<type>float</type>
			<default>3</default>
			<locale name="C">
				<short>Distance threshold for distance based ramble mode for multiple key presses.</short>
				<long>When ramble input method is selected, this the distance the pointer must move over a key (relative to the size of the key) for pressing a key a second time and more.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/behaviour/ramble_timer</key>
			<applyto>/apps/florence/behaviour/ramble_timer</applyto>
			<owner>florence</owner>
			<type>float</type>
			<default>300</default>
			<locale name="C">
				<short>Time threshold for timer based ramble mode.</short>
				<long>Amount of time in milliseconds before the key is automatically pressed.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/behaviour/ramble_button</key>
			<applyto>/apps/florence/behaviour/ramble_button</applyto>
			<owner>florence</owner>
			<type>bool</type>
			<default>true</default>
			<locale name="C">
				<short>Use button to activate rambling.</short>
				<long>When this option is set, rambling starts on button push. No button is necessary when this option is unset.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/behaviour/startup_notification</key>
			<applyto>/apps/florence/behaviour/startup_notification</applyto>
			<owner>florence</owner>
			<type>bool</type>
			<default>true</default>
			<locale name="C">
				<short>Display a startup notification message.</short>
				<long>Display a notification message at startup explaining the basics.</long>
			</locale>
		</schema>

		<schema>
			<key>/schemas/apps/florence/window/xpos</key>
			<applyto>/apps/florence/window/xpos</applyto>
			<owner>florence</owner>
			<type>int</type>
			<default>0</default>
			<locale name="C">
				<short>Position along the X axis</short>
				<long>Horizontal position of the keyboard window on the screen (in pixel).</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/window/ypos</key>
			<applyto>/apps/florence/window/ypos</applyto>
			<owner>florence</owner>
			<type>int</type>
			<default>0</default>
			<locale name="C">
				<short>Position along the Y axis</short>
				<long>Vertical position of the keyboard window on the screen (in pixel).</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/window/zoom</key>
			<applyto>/apps/florence/window/zoom</applyto>
			<owner>florence</owner>
			<type>float</type>
			<default>20</default>
			<locale name="C">
				<short>Scaling factor of the window</short>
				<long>Sets the size of the window and of the keys</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/window/transparent</key>
			<applyto>/apps/florence/window/transparent</applyto>
			<owner>florence</owner>
			<type>bool</type>
			<default>false</default>
			<locale name="C">
				<short>Transparent mode</short>
				<long>To see the windows behind the keyboard.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/window/decorated</key>
			<applyto>/apps/florence/window/decorated</applyto>
			<owner>florence</owner>
			<type>bool</type>
			<default>true</default>
			<locale name="C">
				<short>Decorate window</short>
				<long>Window has a title bar and buttons.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/window/resizable</key>
			<applyto>/apps/florence/window/resizable</applyto>
			<owner>florence</owner>
			<type>bool</type>
			<default>true</default>
			<locale name="C">
				<short>Allow to resize window.</short>
				<long>The keyboard window can be resized with the mouse.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/window/keep_ratio</key>
			<applyto>/apps/florence/window/keep_ratio</applyto>
			<owner>florence</owner>
			<type>bool</type>
			<default>true</default>
			<locale name="C">
				<short>Keep window ratio when resizing window.</short>
				<long>The window proportions are kept intact while resizing the window.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/window/task_bar</key>
			<applyto>/apps/florence/window/task_bar</applyto>
			<owner>florence</owner>
			<type>bool</type>
			<default>false</default>
			<locale name="C">
				<short>Appear in the task bar</short>
				<long>Make Florence appear in the task bar.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/window/always_on_top</key>
			<applyto>/apps/florence/window/always_on_top</applyto>
			<owner>florence</owner>
			<type>bool</type>
			<default>true</default>
			<locale name="C">
				<short>Keep the keyboard on top.</short>
				<long>Keep the keyboard window on top of other windows.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/window/keep_on_top</key>
			<applyto>/apps/florence/window/keep_on_top</applyto>
			<owner>florence</owner>
			<type>bool</type>
			<default>true</default>
			<locale name="C">
				<short>Bring back the keyboard to top.</short>
				<long>Periodically (each second) bring back the keyboard to top. This is useful for window managers not supporting the always_on_top hint.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/window/opacity</key>
			<applyto>/apps/florence/window/opacity</applyto>
			<owner>florence</owner>
			<type>float</type>
			<default>100</default>
			<locale name="C">
				<short>Opacity of the keyboard window</short>
				<long>Sets the opacity of the keyboard window. Requires window manager compositing to work</long>
			</locale>
		</schema>

		<schema>
			<key>/schemas/apps/florence/colours/key</key>
			<applyto>/apps/florence/colours/key</applyto>
			<owner>florence</owner>
			<type>string</type>
			<default>#000000</default>
			<locale name="C">
				<short>key color</short>
				<long>Color of the keys</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/colours/outline</key>
			<applyto>/apps/florence/colours/outline</applyto>
			<owner>florence</owner>
			<type>string</type>
			<default>#808080</default>
			<locale name="C">
				<short>Outline color</short>
				<long>Color of the outline of the keys</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/colours/label</key>
			<applyto>/apps/florence/colours/label</applyto>
			<owner>florence</owner>
			<type>string</type>
			<default>#FFFFFF</default>
			<locale name="C">
				<short>Text color</short>
				<long>Color of the labels of the keys</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/colours/label_outline</key>
			<applyto>/apps/florence/colours/label_outline</applyto>
			<owner>florence</owner>
			<type>string</type>
			<default>#000000</default>
			<locale name="C">
				<short>Text outline color</short>
				<long>Color of the ouline of the labels. Must contrast with label color for better visibility.</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/colours/activated</key>
			<applyto>/apps/florence/colours/activated</applyto>
			<owner>florence</owner>
			<type>string</type>
			<default>#FF0000</default>
			<locale name="C">
				<short>Activated color</short>
				<long>Color of the pressed keys</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/colours/mouseover</key>
			<applyto>/apps/florence/colours/mouseover</applyto>
			<owner>florence</owner>
			<type>string</type>
			<default>#0000FF</default>
			<locale name="C">
				<short>Mouse over color</short>
				<long>Color of the key when the mouse is over</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/colours/latched</key>
			<applyto>/apps/florence/colours/latched</applyto>
			<owner>florence</owner>
			<type>string</type>
			<default>#00FF00</default>
			<locale name="C">
				<short>latched color</short>
				<long>Color of the latched keys</long>
			</locale>
		</schema>

		<schema>
			<key>/schemas/apps/florence/layout/file</key>
			<applyto>/apps/florence/layout/file</applyto>
			<owner>florence</owner>
			<type>string</type>
			<default></default>
			<locale name="C">
				<short>Path of the layout file</short>
				<long>The layout file contains the layout of the keys on the keyboard</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/layout/style</key>
			<applyto>/apps/florence/layout/style</applyto>
			<owner>florence</owner>
			<type>string</type>
			<default></default>
			<locale name="C">
				<short>Path of the layout style file</short>
				<long>The layout style file contains the style to draw the keys of the keyboard</long>
			</locale>
		</schema>
		<schema>
			<key>/schemas/apps/florence/layout/extensions</key>
			<applyto>/apps/florence/layout/extensions</applyto>
			<owner>florence</owner>
			<type>string</type>
			<default>actionkys</default>
			<locale name="C">
				<short>List of colon-separated (:) extension names</short>
				<long>List of keyboard extensions names (colon-separated). extension names must be found in layout file</long>
			</locale>
		</schema>

		<schema>
			<key>/schemas/apps/florence/style/focus_zoom</key>
			<applyto>/apps/florence/style/focus_zoom</applyto>
			<owner>florence</owner>
			<type>float</type>
			<default>1.3</default>
			<locale name="C">
				<short>scaling factor of the focused key</short>
				<long>scaling factor of the focused key</long>
			</locale>
		</schema>

	</schemalist>
</gconfschemafile>
